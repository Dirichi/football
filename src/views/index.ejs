<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <form action="/search" method="post">
      <input type="text" name="name" value="">
      <select name="preferredRoleType">
        <% roleTypes.forEach((value, representation) => { %>
          <option value=<%= value %>><%= representation %></option>
         <% }); %>
      </select>
      <input type="submit">
    </form>
  </body>
</html>
